SCRIPTDIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" >/dev/null 2>&1 && pwd )"   

echo "Note, script updated to use new features in $SCRIPTDIR/show-current-images" 
echo "use $SCRIPTDIR/show-current-images update to find images and only update the files (no commit)"
echo "use $SCRIPTDIR/show-current-images push to find images and commit and push the changes"   
$SCRIPTDIR/show-current-images  update
 